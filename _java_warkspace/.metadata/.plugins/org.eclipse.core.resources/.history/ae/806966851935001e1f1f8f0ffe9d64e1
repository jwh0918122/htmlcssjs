package 프로그래머스;

public class 프로그래머스 {
	public static void main(String[] args) {
		Solution s = new Solution();
		System.out.println(s.solution(3, 10));

	}
}

class Solution {
	public int[] solution(int start, int end) {
		int[] answer = new int[(end - start) + 1];
		int cnt = 0;

		for (int i = start; i <= end; i++) {
			answer[cnt] = i;
			cnt++;
		}

		return answer;
	}
}